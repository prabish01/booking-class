{
  "kind": "collectionType",
  "collectionName": "class_occurrences",
  "info": {
    "singularName": "class-occurrence",
    "pluralName": "class-occurrences",
    "displayName": "Class Occurrence",
    "description": "Specific date/time occurrences for classes that users can book"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "datetime",
      "required": true
    },
    "durationMinutes": {
      "type": "integer",
      "default": 60,
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "songThumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    },
    "spotsAvailable": {
      "type": "integer"
    },
    "externalVideoIds": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::video.video"
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "classOccurrence"
    }
  }
}
