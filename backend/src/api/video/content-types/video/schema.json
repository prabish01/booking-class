{
  "kind": "collectionType",
  "collectionName": "videos",
  "info": {
    "singularName": "video",
    "pluralName": "videos",
    "displayName": "Video",
    "description": "Videos for the dance classes"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "videoUrl": {
      "type": "string",
      "required": true
    },
    "thumbnailUrl": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "duration": {
      "type": "integer"
    },
    "level": {
      "type": "enumeration",
      "enum": ["BEGINNER", "INTERMEDIATE", "ADVANCED"],
      "default": "BEGINNER",
      "required": true
    },
    "isPublished": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
